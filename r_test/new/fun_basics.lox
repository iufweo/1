fun fact(n)
{
	if (n == 0)
		return 1;
	return n * fact(n - 1);
}

fun sum(a, b)
{
	return a + b;
}

fun isPrime(x)
{
	var i;
	var lim;

	for (i = 2, lim = x / 2; i < lim; i = i + 1) {
		if (x % i == 0)
			return false;
	}

	return true;
}

